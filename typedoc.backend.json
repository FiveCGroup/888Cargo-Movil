{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "./888Cris-MERN/backend/controllers",
    "./888Cris-MERN/backend/models",
    "./888Cris-MERN/backend/routes",
    "./888Cris-MERN/backend/services",
    "./888Cris-MERN/backend/middlewares",
    "./888Cris-MERN/backend/utils",
    "./888Cris-MERN/backend/validators"
  ],
  "entryPointStrategy": "expand",
  "out": "./code-docs/backend",
  "tsconfig": "./888Cris-MERN/backend/tsconfig.json",
  "exclude": [
    "**/node_modules/**",
    "**/*.test.js",
    "**/*.spec.js",
    "**/__tests__/**",
    "**/dist/**",
    "**/build/**",
    "**/coverage/**"
  ],
  "excludePrivate": false,
  "excludeProtected": false,
  "excludeExternals": true,
  "includeVersion": true,
  "name": "888 Cargo - Backend API",
  "categorizeByGroup": true,
  "categoryOrder": [
    "Controllers",
    "Services",
    "Models",
    "Routes",
    "Middlewares",
    "Validators",
    "Utils",
    "*"
  ],
  "navigationLinks": {
    "Inicio": "../index.html",
    "Web": "../web/index.html",
    "Mobile": "../mobile/index.html"
  },
  "sort": ["source-order"],
  "plugin": [],
  "jsDocCompatibility": {
    "defaultTag": true,
    "exampleTag": true,
    "ignoreUnescapedBraces": true
  }
}
